<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="AslisGtkAddTernakModal" parent="GtkApplicationWindow">
    <property name="default-width">600</property>
    <property name="default-height">300</property>
    <property name="child">
      <object class="GtkBox">
        <style>
          <class name="body" />
        </style>
        <property name="orientation">vertical</property>
        <child>
        <object class="GtkBox">
          <style>
            <class name="main" />
          </style>
          <property name="orientation">vertical</property>
          <child>
            <object class="GtkButton" id="cancelButton">
              <style>
                <class name="cancel" />
              </style>
              <property name="name">cancel-button</property>
              <property name="label">Cancel</property>
            </object>
          </child>
          <child>
            <object class="GtkGrid" id="grid">
              <style>
                <class name="form" />
              </style>
              <property name="visible">True</property>
              <property name="can-focus">True</property>
              <property name="margin-start">12</property>
              <property name="margin-end">12</property>
              <property name="margin-top">12</property>
              <property name="margin-bottom">12</property>
              <property name="column-spacing">6</property>
              <property name="row-spacing">6</property>
              <property name="column-homogeneous">True</property>
              <property name="row-homogeneous">True</property>

              <!-- Nama Ternak Label -->
              <child>
                <object class="GtkLabel" id="namaTernakLabel">
                  <property name="visible">True</property>
                  <property name="can-focus">False</property>
                  <property name="label" translatable="yes">Nama Ternak:</property>
                  <layout>
                    <property name="column">0</property>
                    <property name="row">0</property>
                    <property name="column-span">1</property>
                    <property name="row-span">1</property>
                  </layout>
                </object>
              </child>

              <!-- Nama Ternak Entry -->
              <child>
                <object class="GtkEntry" id="namaTernakEntry">
                  <property name="visible">True</property>
                  <property name="can-focus">True</property>
                  <property name="margin-start">5</property>
                  <property name="editable">True</property>
                  <layout>
                    <property name="column">1</property>
                    <property name="row">0</property>
                    <property name="column-span">1</property>
                    <property name="row-span">1</property>
                  </layout>
                </object>
              </child>

              <!-- Umur Ternak Label -->
              <child>
                <object class="GtkLabel" id="umurTernakLabel">
                  <property name="visible">True</property>
                  <property name="can-focus">False</property>
                  <property name="label" translatable="yes">Umur Ternak:</property>
                  <layout>
                    <property name="column">0</property>
                    <property name="row">1</property>
                    <property name="column-span">1</property>
                    <property name="row-span">1</property>
                  </layout>
                </object>
              </child>

              <!-- Umur Ternak Entry -->
              <child>
                <object class="GtkEntry" id="umurTernakEntry">
                  <property name="visible">True</property>
                  <property name="can-focus">True</property>
                  <property name="margin-start">5</property>
                  <property name="editable">True</property>
                  <layout>
                    <property name="column">1</property>
                    <property name="row">1</property>
                    <property name="column-span">1</property>
                    <property name="row-span">1</property>
                  </layout>
                </object>
              </child>

              <!-- Berat Ternak Label -->
              <child>
                <object class="GtkLabel" id="beratTernakLabel">
                  <property name="visible">True</property>
                  <property name="can-focus">False</property>
                  <property name="label" translatable="yes">Berat Ternak:</property>
                  <layout>
                    <property name="column">0</property>
                    <property name="row">2</property>
                    <property name="column-span">1</property>
                    <property name="row-span">1</property>
                  </layout>
                </object>
              </child>

              <!-- Berat Ternak Entry -->
              <child>
                <object class="GtkEntry" id="beratTernakEntry">
                  <property name="visible">True</property>
                  <property name="can-focus">True</property>
                  <property name="margin-start">5</property>
                  <property name="editable">True</property>
                  <layout>
                    <property name="column">1</property>
                    <property name="row">2</property>
                    <property name="column-span">1</property>
                    <property name="row-span">1</property>
                  </layout>
                </object>
              </child>

              <!-- Gender Ternak Label -->
              <child>
                <object class="GtkLabel" id="genderTernakLabel">
                  <property name="visible">True</property>
                  <property name="can-focus">False</property>
                  <property name="label" translatable="yes">Gender Ternak:</property>
                  <layout>
                    <property name="column">0</property>
                    <property name="row">3</property>
                    <property name="column-span">1</property>
                    <property name="row-span">1</property>
                  </layout>
                </object>
              </child>


              <!-- Gender Ternak ComboBoxText -->
              <child>
                <object class="GtkComboBoxText" id="genderTernakComboBox">
                  <property name="visible">True</property>
                  <property name="can-focus">True</property>
                  <property name="margin-start">5</property>
                  <items>
                    <item translatable="yes">Pilih</item>
                    <item translatable="yes">Jantan</item>
                    <item translatable="yes">Betina</item>
                  </items>
                  <layout>
                    <property name="column">1</property>
                    <property name="row">3</property>
                    <property name="column-span">1</property>
                    <property name="row-span">1</property>
                  </layout>
                </object>
              </child>


              <!-- Tempat Lahir Ternak Label -->
              <child>
                <object class="GtkLabel" id="tempatLahirTernakLabel">
                  <property name="visible">True</property>
                  <property name="can-focus">False</property>
                  <property name="label" translatable="yes">Tempat Lahir Ternak:</property>
                  <layout>
                    <property name="column">0</property>
                    <property name="row">4</property>
                    <property name="column-span">1</property>
                    <property name="row-span">1</property>
                  </layout>
                </object>
              </child>

              <!-- Tempat Lahir Ternak Entry -->
              <child>
                <object class="GtkEntry" id="tempatLahirTernakEntry">
                  <property name="visible">True</property>
                  <property name="can-focus">True</property>
                  <property name="margin-start">5</property>
                  <property name="editable">True</property>
                  <layout>
                    <property name="column">1</property>
                    <property name="row">4</property>
                    <property name="column-span">1</property>
                    <property name="row-span">1</property>
                  </layout>
                </object>
              </child>

              <!-- Create Button -->
              <child>
                <object class="GtkButton" id="createButton">
                  <style>
                    <class name="create" />
                  </style>
                  <property name="label" translatable="yes">Create</property>
                  <property name="visible">True</property>
                  <property name="can-focus">True</property>
                  <layout>
                    <property name="column">0</property>
                    <property name="row">5</property>
                    <property name="column-span">2</property>
                    <property name="row-span">1</property>
                  </layout>
                </object>
              </child>

              <!-- Result Label -->
              <child>
                <object class="GtkLabel" id="resultLabel">
                  <property name="visible">True</property>
                  <property name="can-focus">False</property>
                  <layout>
                    <property name="column">0</property>
                    <property name="row">6</property>
                    <property name="column-span">2</property>
                    <property name="row-span">1</property>
                  </layout>
                </object>
              </child>
            </object>
          </child>
        </object>
      </child>
      </object>
    </property>
  </template>
</interface>